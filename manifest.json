{
    "manifest_version": 2,
    "version": "1.20.622",
    "name": "Full-Screen agile board mode for Jira",
    "author":"Rui Costa",
    "homepage_url": "https://github.com/rui-costa/jira_fullscreen_board",
    "content_scripts": [
        {
          "matches": ["https://*.atlassian.net/secure/RapidBoard.jspa*"],
          "run_at": "document_end",
          "js": ["extension.js"]
        }
      ],
      "icons": { "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png" }
  }